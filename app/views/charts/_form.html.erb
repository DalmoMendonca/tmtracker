<%= simple_form_for(@chart) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :name, :collection => %w[CC ACB ACS ACG CL ALB ALS] %>
    <%= hidden_field_tag(:member_id, @chart.member_id) %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
